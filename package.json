{
  "name": "react-offscreen",
  "version": "1.0.3",
  "description": "React component for offscreen text",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "lint": "eslint .",
    "release": "standard-version",
    "test": "nyc npm run test:run && npm run lint",
    "test:run": "tape -r babel-register test/index.js | tap-spec",
    "start": "npm run build && webpack-dev-server --config webpack.demo.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schne324/react-offscreen.git"
  },
  "keywords": [
    "a11y",
    "react",
    "offscreen"
  ],
  "author": "Harris Schneiderman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schne324/react-offscreen/issues"
  },
  "homepage": "https://github.com/schne324/react-offscreen#readme",
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "3.0.2",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "standard-version": "^4.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
